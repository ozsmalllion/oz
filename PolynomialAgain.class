package chapter4;

import java.util.Scanner;
/**
 * 
 * @author ozsmalllion
 *
 */
public class PolynomialAgain {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);		
		int[] first=new int[101];
		int polynomials = 0;
		int data1;
		int data2;
		int low = 100;
		while(true){
			data1 = in.nextInt();
			data2 = in.nextInt();
			if(data1 == 0 ){
				first[data1]=first[data1] + data2;
				polynomials++;	
			}else{
				first[data1]=first[data1] + data2;
			}
			if(polynomials == 2){
				break;
			}
		}			
		for(int i = first.length - 1;i>=0;i--){
			if(first[i] != 0){
				low = i;
			}			
		}
		for(int i = first.length - 1;i>=0;i--){
			if(i != low){
				if(first[i] == 1){
					if(i==0){
						System.out.print( "1");
					}else if(i==1){
						System.out.print( "x"  + "+");
					}else{
						System.out.print( "x" + i + "+");
					}
				}else if(first[i] == -1){
					if(i==0){
						System.out.print( "-1");
					}else if(i==1){
						System.out.print( "-" + "x"  + "+");
					}else{
						System.out.print( "-" + "x" + i + "+");
					}
				}else if(first[i] == 0){
					//System.out.print( "");
				}else{
					if(i==0){
						System.out.print( first[i] + "+");
					}else if(i==1){
						System.out.print( first[i]  + "x"  + "+");
					}else{
						System.out.print(first[i] + "x" + i + "+");
					}				
				}
			}else{
				if(first[i] == 1){
					if(i==0){
						System.out.print( "1");
					}else if(i==1){
						System.out.print( "x"  );
					}else{
						System.out.print( "x" + i );
					}
				}else if(first[i] == -1){
					if(i==0){
						System.out.print( "-1");
					}else if(i==1){
						System.out.print( "-" + "x"  );
					}else{
						System.out.print( "-" + "x" + i );
					}
				}else if(first[i] == 0){
					System.out.print( "0");
				}else{
					if(i==0){
						System.out.print( first[i] );
					}else if(i==1){
						System.out.print( first[i]  + "x"  );
					}else{
						System.out.print(first[i] + "x" + i );
					}				
				}
			}
		}		
		in.close();
	}
}
